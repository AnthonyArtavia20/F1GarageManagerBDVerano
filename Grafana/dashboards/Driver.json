      "targets": [
        {
          "dataset": "F1GarageManager",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    sp.position as Position,\n    FORMAT(s.Data_time, 'dd/MM/yyyy HH:mm') as Date,\n    c.Name as Circuit,\n    CONCAT(\n        FLOOR(sp.time_seconds / 60), 'm ',\n        ROUND(sp.time_seconds % 60, 2), 's'\n    ) as Time,\n    ROUND(sp.v_recta, 1) as 'Straight Speed',\n    ROUND(sp.v_curva, 1) as 'Curve Speed',\n    ROUND(sp.penalty, 1) as Penalty,\n    t.Name as Team\nFROM SIMULATION_PARTICIPANT sp\nJOIN SIMULATION s ON sp.simulation_id = s.Simulation_id\nJOIN CIRCUIT c ON s.Circuit_id = c.Circuit_id\nJOIN TEAM t ON sp.team_id = t.Team_id\nJOIN CAR car ON sp.car_id = car.Car_id\nWHERE sp.driver_id = ${Driver}\nORDER BY s.Data_time DESC;",
          "refId": "A",
