      "targets": [
        {
          "dataset": "F1GarageManager",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH UsedParts AS (\n    SELECT \n        ssd.category AS Category,\n        p.Name AS Name,\n        AVG(ssd.part_p) AS p,\n        AVG(ssd.part_a) AS a,\n        AVG(ssd.part_m) AS m,\n        AVG(sp.position) AS [Average Position],\n        MIN(sp.position) AS [Best Position]\n    FROM SIMULATION_PARTICIPANT sp\n    JOIN SIMULATION_SETUP_DETAIL ssd \n        ON sp.simulation_id = ssd.simulation_id \n       AND sp.car_id = ssd.car_id\n    JOIN PART p ON ssd.part_id = p.Part_id\n    WHERE sp.team_id = ${Team}\n      AND (${TeamCar} = 0 OR sp.car_id = ${TeamCar})\n    GROUP BY ssd.category, p.Name\n)\nSELECT \n    Category,\n    Name,\n    p,\n    a,\n    m,\n    [Average Position],\n    [Best Position]\nFROM UsedParts;",
          "refId": "A",
